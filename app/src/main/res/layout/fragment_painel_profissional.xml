<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:layout_editor_absoluteY="25dp">


    <LinearLayout
        android:paddingTop="32dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

            <TextView

                android:textAlignment="center"
                android:textSize="25sp"
                android:padding="16dp"
                android:layout_marginRight="16dp"
                android:layout_marginLeft="16dp"
                android:background="@drawable/background_null"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:text="Painel Administrativo" />



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="16dp"
            android:gravity="center_horizontal"
            android:layout_gravity="center"
            android:paddingTop="32dp">



            <RelativeLayout
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginRight="32dp"
                android:background="@drawable/background_null">


                <ImageView
                    android:id="@+id/img_Solicitar_Coleta"
                    android:layout_width="62dp"
                    android:layout_height="58dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="12dp"
                    android:src="@drawable/ic_prancheta" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="20dp"
                    android:text="Financeiro" />


            </RelativeLayout>


            <RelativeLayout
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginRight="32dp"
                android:background="@drawable/background_null">


                <ImageView
                    android:id="@+id/img_Historico_Coleta"
                    android:layout_width="62dp"
                    android:layout_height="58dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="12dp"
                    android:src="@drawable/ic_prancheta" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="20dp"
                    android:text="meus clientes" />




            </RelativeLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:gravity="center_horizontal"
            android:layout_gravity="center"
            android:paddingTop="32dp">


            <RelativeLayout
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginRight="32dp"
                android:background="@drawable/background_null">


                <ImageView
                    android:id="@+id/img_Minhas_Coletas"
                    android:layout_width="62dp"
                    android:layout_height="58dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="12dp"
                    android:src="@drawable/ic_prancheta"  />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="20dp"
                    android:text="Meus Pedidos" />




            </RelativeLayout>


            <RelativeLayout
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginRight="32dp"
                android:background="@drawable/background_null">


                <ImageView
                    android:src="@drawable/ic_prancheta"

                    android:id="@+id/img_Promocoes"
                    android:layout_width="62dp"
                    android:layout_height="58dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="12dp"/>

                <TextView
                    android:textAlignment="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="10dp"
                    android:text="Meus Produtos/ServiÃ§os" />




            </RelativeLayout>





        </LinearLayout>





    </LinearLayout>



</LinearLayout>





