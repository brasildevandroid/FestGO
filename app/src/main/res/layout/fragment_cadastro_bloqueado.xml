<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:id="@+id/relative"
        android:layout_width="match_parent"
        android:layout_height="70dp">


        <TextView
            android:layout_centerInParent="true"
            android:id="@+id/txt_Fornecedores"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:drawableTop="@drawable/ic_fornecedores"
            android:text="Fornecedores"
            android:textSize="15sp" />


        <TextView
            android:id="@+id/txt_Pagamentos"
            android:layout_centerInParent="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="20dp"
            android:drawableTop="@drawable/ic_pagamento"
            android:text="Pagamentos"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/txt_Salvos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:drawableTop="@drawable/ic_salvos"
            android:text="Salvos"
            android:textSize="15sp" />



        <View
            android:id="@+id/linha_Fornecedor"
            android:layout_width="90dp"
            android:layout_height="3dp"
            android:layout_alignStart="@+id/txt_Fornecedores"
            android:layout_below="@+id/txt_Fornecedores"
            android:background="@color/colorAccent" />


        <View

            android:id="@+id/linha_Pagamento"
            android:layout_width="90dp"
            android:layout_height="3dp"
            android:layout_alignEnd="@+id/txt_Pagamentos"
            android:layout_alignParentBottom="true"
            android:background="@color/colorAccent" />


        <View
            android:id="@+id/linha_Salvos"
            android:layout_width="90dp"
            android:layout_height="3dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:background="@color/colorAccent" />

    </RelativeLayout>


